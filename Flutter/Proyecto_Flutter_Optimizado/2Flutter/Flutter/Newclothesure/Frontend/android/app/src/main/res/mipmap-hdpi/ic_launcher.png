<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Gradiente del fondo cuadrado -->
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B1538;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#A01A42;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B1538;stop-opacity:1" />
    </linearGradient>
    
    <!-- Gradiente del círculo central -->
    <radialGradient id="circleGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#4A0E2A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2D0819;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Fondo cuadrado con esquinas redondeadas -->
  <rect x="20" y="20" width="472" height="472" rx="40" ry="40" fill="url(#bgGradient)"/>
  
  <!-- Círculo central morado -->
  <circle cx="256" cy="200" r="80" fill="url(#circleGradient)"/>
  
  <!-- Perchas estilizadas -->
  <!-- Percha central con corazón -->
  <path d="M256,140 C256,140 230,120 220,130 C210,140 210,160 220,170 C230,180 256,200 256,200 C256,200 282,180 292,170 C302,160 302,140 292,130 C282,120 256,140 256,140 Z" fill="#F5F5DC" stroke="#E6E6D4" stroke-width="1"/>
  
  <!-- Percha central - poste -->
  <line x1="256" y1="200" x2="256" y2="280" stroke="#F5F5DC" stroke-width="4" stroke-linecap="round"/>
  <line x1="220" y1="260" x2="292" y2="260" stroke="#F5F5DC" stroke-width="4" stroke-linecap="round"/>
  
  <!-- Percha izquierda -->
  <line x1="200" y1="180" x2="200" y2="260" stroke="#F5F5DC" stroke-width="3" stroke-linecap="round"/>
  <line x1="170" y1="240" x2="230" y2="240" stroke="#F5F5DC" stroke-width="3" stroke-linecap="round"/>
  
  <!-- Percha derecha -->
  <line x1="312" y1="180" x2="312" y2="260" stroke="#F5F5DC" stroke-width="3" stroke-linecap="round"/>
  <line x1="282" y1="240" x2="342" y2="240" stroke="#F5F5DC" stroke-width="3" stroke-linecap="round"/>
  
  <!-- Texto "The Clothesure" -->
  <text x="256" y="360" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#F5F5DC">The</text>
  <text x="256" y="390" text-anchor="middle" font-family="Arial, sans-serif" font-size="28" font-weight="bold" fill="#F5F5DC">Clothesure</text>
</svg>
